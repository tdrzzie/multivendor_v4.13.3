msgid "Previous rating"
msgstr "以前の評価"

msgctxt "Languages::vendor_rating.rating"
msgid "Rating"
msgstr "評価"

msgctxt "Languages::vendor_rating.vendor_rating"
msgid "Vendor rating"
msgstr "出品者評価"

msgctxt "Languages::sort_by_absolute_vendor_rating_asc"
